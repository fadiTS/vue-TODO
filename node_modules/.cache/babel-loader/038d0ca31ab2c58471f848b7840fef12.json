 if (this.newTodo.trim().length == 0) {\n        return;\n      }\n\n      this.todos.push({\n        id: this.idForTodo,\n        title: this.newTodo,\n        completed: false,\n        editing: false\n      });\n      this.saveTodo();\n      this.newTodo = '';\n      this.idForTodo++;\n    },\n    editTodo: function editTodo(todo) {\n      this.beforeEditCache = todo.title;\n      todo.editing = true;\n      this.saveTodo();\n    },\n    doneEdit: function doneEdit(todo) {\n      if (todo.title.trim() == '') {\n        todo.title = this.beforeEditCache;\n      }\n\n      todo.editing = false;\n      this.saveTodo();\n    },\n    cancelEdit: function cancelEdit(todo) {\n      todo.title = this.beforeEditCache;\n      todo.editing = false;\n      this.saveTodo();\n    },\n    removeTodo: function removeTodo(index) {\n      this.todos.splice(index, 1);\n      this.saveTodo();\n    },\n    checkAllTodos: function checkAllTodos() {\n      this.todos.forEach(function (todo) {\n        return todo.completed = event.target.checked;\n      });\n      this.saveTodo();\n    },\n    clearCompleted: function clearCompleted() {\n      this.todos = this.todos.filter(function (todo) {\n        return !todo.completed;\n      });\n      this.saveTodo();\n    },\n    clearCache: function clearCache() {\n      localStorage.removeItem('todos');\n      this.setTodod();\n    },\n    setTodod: function setTodod() {\n      this.todos = [{\n        'id': 1,\n        'title': 'Save TODO date to persistent local storage. ',\n        'completed': true,\n        'editing': false\n      }, {\n        'id': 2,\n        'title': 'Finish TODO Application',\n        'completed': false,\n        'editing': false\n      }, {\n        'id': 3,\n        'title': 'Take over world',\n        'completed': false,\n        'editing': false\n      }];\n    },\n    saveTodo: function saveTodo() {\n      var todosParsed = JSON.stringify(this.todos);\n      localStorage.setItem('todos', todosParsed);\n    }\n  },\n  created: function created() {\n    var _this = this;\n\n    // Using the service bus\n    serverBus.$on('lang', function (lang) {\n      _this.lang = lang;\n    });\n  },\n  mounted: function mounted() {\n    var app = this;\n    window.addEventListener('keyup', function (event) {\n      if (event.keyCode === 13) {\n        app.addTodo();\n      }\n    });\n\n    if (localStorage.lang) {\n      this.lang = localStorage.lang;\n    } else {\n      this.lang = 'EN';\n    }\n\n    if (localStorage.getItem('todos')) {\n      try {\n        this.todos = JSON.parse(localStorage.getItem('todos'));\n      } catch (e) {\n        localStorage.removeItem('todos');\n      }\n    } else {\n      this.setTodod();\n    }\n  },\n  watch: {\n    lang: function lang(newLang) {\n      localStorage.lang = newLang;\n    }\n  }\n};",{"version":3,"sources":["main.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqDA,SAAA,SAAA,QAAA,SAAA;AAGA,eAAA;AACA,QAAA,WADA;AAEA,MAFA,kBAEA;AACA,WAAA;AACA,eAAA,EADA;AAEA,iBAAA,CAFA;AAGA,uBAAA,EAHA;AAIA,cAAA,KAJA;AAKA,aAAA,EALA;AAMA,YAAA,IANA;AAOA,mBAAA;AACA,cAAA;AACA,iBAAA,KADA;AAEA,yBAAA,iBAFA;AAGA,iBAAA,KAHA;AAIA,oBAAA,QAJA;AAKA,uBAAA,WALA;AAMA,mBAAA,iBANA;AAOA,kBAAA,MAPA;AAQA,uBAAA,YARA;AASA,sBAAA,WATA;AAUA,mBAAA;AAVA,SADA;AAaA,cAAA;AACA,iBAAA,QADA;AAEA,yBAAA,uBAFA;AAGA,iBAAA,OAHA;AAIA,oBAAA,QAJA;AAKA,uBAAA,WALA;AAMA,mBAAA,oBANA;AAOA,kBAAA,SAPA;AAQA,uBAAA,mBARA;AASA,sBAAA,gBATA;AAUA,mBAAA;AAVA,SAbA;AAyBA,cAAA;AACA,iBAAA,WADA;AAEA,yBAAA,uBAFA;AAGA,iBAAA,KAHA;AAIA,oBAAA,UAJA;AAKA,uBAAA,aALA;AAMA,mBAAA,oBANA;AAOA,kBAAA,QAPA;AAQA,uBAAA,oBARA;AASA,sBAAA,eATA;AAUA,mBAAA;AAVA;AAzBA;AAPA,KAAA;AA8CA,GAjDA;AAkDA,YAAA;AACA,aADA,uBACA;AACA,aAAA,KAAA,KAAA,CAAA,MAAA,CAAA;AAAA,eAAA,CAAA,KAAA,SAAA;AAAA,OAAA,EAAA,MAAA;AACA,KAHA;AAIA,gBAJA,0BAIA;AACA,aAAA,KAAA,SAAA,IAAA,CAAA;AACA,KANA;AAOA,iBAPA,2BAOA;AACA,UAAA,KAAA,MAAA,IAAA,KAAA,EAAA;AACA,eAAA,KAAA,KAAA;AACA,OAFA,MAEA,IAAA,KAAA,MAAA,IAAA,QAAA,EAAA;AACA,eAAA,KAAA,KAAA,CAAA,MAAA